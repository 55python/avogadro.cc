---
sitemap: false
---
[
  {% for node in site.pages %}
    {
  	{% if node.title != nil and node.url != '/' %}
        "title"   : "{{ node.title | escape }}",
        "category": "{{ node.categories }}",
        "url"     : "{{ site.url }}{{ node.url }}"
        "excerpt" : "{{ node.content | strip_html | truncatewords:75}}"
	  {% endif %}
    } {% unless forloop.last %},{% endunless %}
  {% endfor %}
]
