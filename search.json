---
sitemap: false
---
[
  {% for node in site.pages %}
    {
  	{% if node.title != nil and node.url != '/' %}
  	{% capture nodediff %}{{ page.url | remove:node.url }}{% endcapture %}
        "title"   : "{{ node.title | escape }}",
        "category": "{{ node.category }}",
        "url"     : "{{ site.url }}{ node.url }}"
	  {% endif %}
    } {% unless forloop.last %},{% endunless %}
  {% endfor %}
]
